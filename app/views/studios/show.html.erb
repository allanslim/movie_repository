<div id="dashboard_bg" class="dashboard_bg"/>
  <br/><br/>
  <div class="container" >
    <div class="well" >


			<p id="notice"><%= notice %></p>

			<p>
			  <strong>Name:</strong>
			  <%= @studio.name %>
			</p>

			<p>
			  <strong>Address:</strong>
			  <%= @studio.address %>
			</p>

			<p>
			  <strong>Telephone:</strong>
			  <%= @studio.telephone %>
			</p>

			<p>
			  <strong>Fax:</strong>
			  <%= @studio.fax %>
			</p>

		    <% if @isAdmin == 1 %>
				<%= link_to 'Edit', edit_studio_path(@studio) %> |
			<% end %>
			<%= link_to 'Back', studios_path %>
	</div>
  </div>
</div>
