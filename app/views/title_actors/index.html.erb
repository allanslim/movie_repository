<h1>Listing title_actors</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Actor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @title_actors.each do |title_actor| %>
      <tr>
        <td><%= title_actor.title_id %></td>
        <td><%= title_actor.actor_id %></td>
        <td><%= link_to 'Show', title_actor %></td>
        <td><%= link_to 'Edit', edit_title_actor_path(title_actor) %></td>
        <td><%= link_to 'Destroy', title_actor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Title actor', new_title_actor_path %>
