<%= form_for(@asset) do |f| %>
  <% if @asset.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@asset.errors.count, "error") %> prohibited this asset from being saved:</h2>

      <ul>
      <% @asset.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :assetValue %><br>
    <%= f.text_field :assetValue %>
  </div>
  <div class="field">
    <%= f.label :assetType %><br>
    <%= f.text_field :assetType %>
  </div>

  <div class="field">
    <%= f.label :title %>
    <%= select_tag 'title_id', options_from_collection_for_select(@titles,'id', 'name', :selected => @selectedTitle), :class => 'form-control'  %>
  </div>


  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
