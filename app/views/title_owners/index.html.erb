<h1>Listing title_owners</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Studio</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @title_owners.each do |title_owner| %>
      <tr>
        <td><%= title_owner.title_id %></td>
        <td><%= title_owner.studio_id %></td>
        <td><%= link_to 'Show', title_owner %></td>
        <td><%= link_to 'Edit', edit_title_owner_path(title_owner) %></td>
        <td><%= link_to 'Destroy', title_owner, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Title owner', new_title_owner_path %>
